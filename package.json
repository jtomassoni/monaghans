{
  "name": "monaghans",
  "version": "1.0.0",
  "description": "Neighborhood dive bar website with owner-friendly CMS",
  "scripts": {
    "dev": "node scripts/show-ip.js && next dev -H 0.0.0.0",
    "build": "node scripts/build.js",
    "build:fast": "SKIP_MIGRATIONS=true node scripts/build.js",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit --incremental false",
    "prisma:validate": "prisma validate",
    "check": "npm run lint && npm run typecheck",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:sync": "tsx scripts/sync-db-schema.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:setup": "tsx scripts/setup-local-db.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:studio": "prisma studio",
    "test:setup": "tsx scripts/setup-test-db.ts",
    "generate-specials": "tsx scripts/generate-daily-specials.ts",
    "set-social": "tsx scripts/set-social-media.ts",
    "fix-taco-platter": "tsx scripts/fix-taco-platter.ts",
    "validate:docs": "tsx scripts/validate-help-docs.ts",
    "validate:docs:report": "tsx scripts/validate-help-docs.ts --markdown --report validation-report.md",
    "validate:docs:json": "tsx scripts/validate-help-docs.ts --json --report validation-report.json",
    "detect:changes": "tsx scripts/detect-feature-changes.ts",
    "detect:changes:save": "tsx scripts/detect-feature-changes.ts --save",
    "detect:changes:report": "tsx scripts/detect-feature-changes.ts --markdown --report change-detection-report.md",
    "suggest:doc-updates": "tsx scripts/suggest-doc-updates.ts",
    "test:help-validation": "tsx scripts/test-help-validation.ts",
    "test:help-quality": "tsx scripts/test-help-quality.ts",
    "test:help": "npm run test:help-validation && npm run test:help-quality"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.19.0",
    "@stripe/react-stripe-js": "^5.3.0",
    "@stripe/stripe-js": "^8.4.0",
    "@types/node": "^24.10.0",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "date-fns": "^4.1.0",
    "marked": "^16.4.1",
    "next": "16.0.10",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.11",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "rrule": "^2.8.1",
    "stripe": "^19.3.0",
    "typescript": "^5.9.3",
    "zod": "^4.1.12",
    "pdfjs-dist": "^4.0.379",
    "sharp": "^0.33.5"
  },
  "optionalDependencies": {
    "canvas": "^2.11.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/marked": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6"
  }
}
